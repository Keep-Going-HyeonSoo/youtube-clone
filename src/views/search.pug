extends layouts/main
include mixins/videoBlock

block content
  .search__container
    .search__videos
      if videos.length === 0
        .search__header
          span '#{searchingBy}' 의 검색결과가 없습니다.
      else
        .search__header
          span '#{searchingBy}' 의 검색결과
        .videos__container
          each item in videos
            +videoBlock({
              id: item.id,
              title: item.title,
              views: item.views,
              videoFile: item.fileUrl
            })