{
  "name": "hyeontube",
  "version": "1.0.0",
  "description": "Cloning Youtube wite VanilaJS and Nodejs",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "debug": "PORT=5858 nodemon --inspect --exec babel-node --debug-brk=5858 init.js --delay 2",
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2",
    "dev:assets": "cd src && WEBPACK_ENV=development webpack -w",
    "build:server": "babel src --out-dir dist --ignore 'src/assets','src/static','src/webpack.config.js'",
    "build:assets": "cd src && WEBPACK_ENV=production webpack",
    "copyAll": "cp -r src/static dist && cp -r src/views dist",
    "prebuild": "rm -rf dist",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "start": "PRODUCTION=true node dist/init.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Keep-Going-HyeonSoo/HyeonTube.git"
